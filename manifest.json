{
  "vendor": "beightone",
  "name": "gohubly-vitrine-digital",
  "version": "0.0.0",
  "title": "Hubly Vitrine Digital",
  "description": "CHANGE_ME",
  "builders": {
    "react": "3.x",
    "messages": "1.x",
    "docs": "0.x",
    "store": "0.x",
    "node": "6.x",
    "graphql": "1.x"
  },
  "dependencies": {
    "vtex.store": "2.x",
    "vtex.search-graphql": "0.x",
    "vtex.search-result": "3.x",
    "vtex.render-runtime": "8.x"
  },
  "registries": ["smartcheckout"],
  "policies": [
    {
      "name": "graphql-query"
    },
    {
      "name": "outbound-access",
